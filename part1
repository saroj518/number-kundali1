from math import sqrt
def fun():

 a=int(input("enter your number"))
 sqr_root = sqrt(a)
 print("square root is",sqr_root)

 cube = a**3
 print("cube",cube)

 sqr = a**2
 print("square",sqr)

 if a % 2 == 0:
    print("even")
 else:
    print("odd")


 fact=1
 for i in range(1,a+1):
    fact =fact*i
 print("factorial of",fact)
fun()